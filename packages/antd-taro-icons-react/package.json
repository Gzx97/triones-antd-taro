{
  "name": "@trionesdev/antd-taro-icons-react",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "files": [
    "dist",
    "es",
    "lib"
  ],
  "sideEffects": [
    "**/*.css",
    "**/index.es.js"
  ],
  "main": "./dist/index.js.",
  "module": "./es/index.es.js",
  "style": "dist/style.css",
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "vite build --watch",
    "build": "tsc && npm run build:umd && npm run build:es && npm run build:cjs",
    "build:es": "tsc && vite build --config vite.es.config.ts",
    "build:cjs": "tsc && vite build --config vite.cjs.config.ts",
    "build:umd": "tsc && vite build --config vite.umd.config.ts"
  },
  "devDependencies": {
    "@types/react": "^18.3.10",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react-swc": "^3.7.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "classnames": "^2.5.1",
    "rollup-plugin-styles": "^4.0.0",
    "typescript": "~5.6.3",
    "vite": "^5.4.9",
    "vite-plugin-dts": "^4.2.4"
  }
}